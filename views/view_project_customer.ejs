<link rel="stylesheet" href="css/view_project.css">
<script src="scripts/vp_eng.js" defer></script>
<script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
<body class="body">
    <center><h1>Solar Project</h1></center>
<div class="bruh">
    <h1>Project Name :</h1>
    <h2><%= name %></h2>
</div>
<div class="bruh">
    <h1>Project Status :</h1>
    <h2><%= status %></h2>
</div>
<div class="bruh">
    <h1>Project Notes :</h1>
    <h2><%= description %></h2>
</div>
<div class="bruh">
    <h1>Project id :</h1>
    <h2><%= _id %></h2>
</div>
<div class="bruh">
    <h1>Customer Name :</h1>
    <h2><%= customerName %></h2>
</div>
<div class="bruh">
    <h1>Customer Email :</h1>
    <h2><%= customerEmail %></h2>
</div>
<div class="bruh">
    <h1>Customer Phone # :</h1>
    <h2><%= phone %></h2>
</div>
<div class="bruh">
    <h1>Property Address: </h1>
    <h2><%= propertyAddress %></h2>
</div>
<div class="bruh">
    <h1>Project Size: </h1>
    <h2><%= projectSize %> sqft</h2>
</div>
<div class="bruh">
    <% if(images) { %>
        <% for (let i = 0; i < images.length; i++) { %>
        <img src="<%= images[i] %>" alt="<%= images[i] %>">
        <% } %>
    <% } %>
</div>
</body>